{
  "defaultSeverity": "error",
  "jsRules": {},
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-console": true,
    "file-name-casing": [
      true,
      "kebab-case"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-inferrable-types": true,
    "object-literal-sort-keys": false,
    "no-submodule-imports": [
      true,
      "uuid"
    ]
  }
}